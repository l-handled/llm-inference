# .env.template for local development
# Copy this file to .env and fill in your secrets

# MongoDB connection URI
MONGODB_URI=mongodb://localhost:27017

# Qdrant URL (e.g., http://localhost:6333)
QDRANT_URL=http://localhost:6333

# LangSmith API token
LANGSMITH_API_KEY=your-langsmith-api-key-here
API_TOKEN=your-langsmith-api-key-here

# AWS region (if using AWS Secrets Manager)
AWS_REGION=your-aws-region

# AWS Secrets Manager secret name (if using cloud secret loading)
AWS_SECRET_NAME=your-aws-secret-name

# Prometheus Pushgateway URL (optional, for metrics)
PROMETHEUS_PUSHGATEWAY_URL=http://localhost:9091

# Add any other secrets or configuration below as needed


